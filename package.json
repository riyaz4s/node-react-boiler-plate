    "colors": "^1.3.0",
    "compression": "^1.7.3",
    "cookie-parser": "^1.4.3",
    "csrf": "^3.0.6",
    "express": "^4.16.3",
    "express-handlebars": "^3.0.0",
    "helmet": "^3.12.1",
    "history": "^4.7.2",
    "humps": "^2.0.1",
    "ioredis": "^3.2.2",
    "joi": "^13.4.0",
    "lodash": "^4.17.10",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-bootstrap": "^1.0.0-beta.5",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.4.1",
    "react-html-parser": "^2.0.2",
    "react-loadable": "^5.5.0",
    "react-modal": "^3.4.5",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^5.0.0-alpha.9",
    "redux": "^4.0.0",
