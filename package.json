{
  "name": "curriculum",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "build": "webpack --config webpack.dev.js",
    "components:create": "./scripts/components/create.sh",
    "production:build": "webpack --config webpack.prod.js --color -p --progress",
    "build:watch": "webpack --config webpack.dev.js --watch --color --progress",
    "eslint": "eslint '{app,client,config,test}/**/*.{js,jsx}'",
    "eslint:fix": "npm run eslint -- --fix",
    "lint": "npm-run-all eslint lint:scss",
    "lint:scss": "sass-lint -q -v",
    "start": "node server",
    "start:all": "run-p start-fake-service start-dev-server",
    "start-dev": "nodemon --exec npm run start:all",
    "start-dev:watch": "cross-env NODE_ENV=local run-p build:watch start-dev",
    "start-dev-server": "cross-env NODE_ENV=local node server.js",
    "start-fake-service": "node fake-services/start.js",
    "start:watch": "npm-run-all build start-dev:watch"
  },
  "keywords": [
    "node",
    "react",
    "redux",
    "saga"
  ],
  "author": "",
  "dependencies": {
    "@material-ui/core": "^3.9.2",
    "@material-ui/icons": "^3.0.2",
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
